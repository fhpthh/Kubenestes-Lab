pipeline {
  agent any
  tools {
    nodejs 'node'
  }
  stages {
    stage('Install') {
      steps {
        dir('lab6/sample') {
          sh 'npm install'
        }
      }
    }

    stage('Test') {
      steps {
        dir('lab6/sample') {
          sh 'npm test'
        }
      }
    }
  }
}